   <children>
      <Button fx:id="uploadButton" layoutX="900.0" layoutY="101.0" mnemonicParsing="false" onAction="#press" prefHeight="30.0" prefWidth="212.0" text="Choose Image " />
      <Canvas fx:id="canvas" height="573.0" layoutX="739.0" layoutY="332.0" width="746.0" />
      <Canvas fx:id="temp" height="573.0" layoutX="739.0" layoutY="332.0" onMouseDragged="#mouseDragged" onMousePressed="#mousePressed" onMouseReleased="#mouseReleased" width="746.0" />
      <Button fx:id="saveButton" layoutX="1090.0" layoutY="905.0" mnemonicParsing="false" onAction="#save" text="Save!" />

   </children>
</AnchorPane>
